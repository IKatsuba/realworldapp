{
  "projectType": "library",
  "sourceRoot": "libs/web/shared/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/shared"],
      "options": {
        "jestConfig": "libs/web/shared/jest.config.ts",
        "passWithNoTests": true
      }
