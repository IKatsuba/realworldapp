{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "@app/api/http/article": ["libs/api/http/article/src/index.ts"],
      "@app/api/http/profile": ["libs/api/http/profile/src/index.ts"],
      "@app/api/http/tag": ["libs/api/http/tag/src/index.ts"],
      "@app/api/http/user": ["libs/api/http/user/src/index.ts"],
      "@app/api/migrations": ["libs/api/migrations/src/index.ts"],
      "@app/api/shared": ["libs/api/shared/src/index.ts"],
      "@app/models": ["libs/models/src/index.ts"],
      "@app/web/core": ["libs/web/core/src/index.ts"],
      "@app/web/pages/article": ["libs/web/pages/article/src/index.ts"],
      "@app/web/pages/auth": ["libs/web/pages/auth/src/index.ts"],
      "@app/web/pages/editor": ["libs/web/pages/editor/src/index.ts"],
      "@app/web/pages/home": ["libs/web/pages/home/src/index.ts"],
      "@app/web/pages/profile": ["libs/web/pages/profile/src/index.ts"],
      "@app/web/pages/settings": ["libs/web/pages/settings/src/index.ts"],
      "@app/web/shared": ["libs/web/shared/src/index.ts"]
    },
    "baseUrl": ".",
    "rootDir": "."
  },
  "exclude": ["node_modules", "tmp"]
}
